{
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"rounded-full": {
		"style": {
			"borderTopLeftRadius": 9999,
			"borderTopRightRadius": 9999,
			"borderBottomRightRadius": 9999,
			"borderBottomLeftRadius": 9999
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-blue-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(191 219 254 / var(--tw-bg-opacity))"
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-1": {
		"style": {
			"paddingTop": 4,
			"paddingBottom": 4
		}
	},
	"pt-12": {
		"style": {
			"paddingTop": 48
		}
	},
	"font-semibold": {
		"style": {
			"fontWeight": "600"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"text-blue-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(30 64 175 / var(--tw-text-opacity))"
		}
	}
}